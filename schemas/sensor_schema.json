{
	"$schema": "http://json-schema.org/draft-06/schema#", 
	"type": "object",
	"properties": {
		"ID": {
			"type": "string",
			"required": true
		},
		"desc": {
			"type": "string"
		},
		"TTN": {
			"type": "object",
			"properties": {
				"AppId": {
					"type": "string"
				},
				"DevId": {
					"type": "string"
				},
				"AccessKey": {
					"type": "string"
				},
				"URL": {
					"type": "string"
				}
			},
			"required": ["AppId", "DevId", "AccessKey"]
		},
		"type": {
			"type": "string",
			"required": true
		},
		"version": {
			"type": "string",
			"required": true
		},
		"tags": {
			"type": "object"
		}
	},
	"required": ["ID", "TTN", "type", "version"]
}